{
    "buildCommand": "cd front_end && npm install && npm run build",
    "outputDirectory": "front_end/dist",
    "framework": null,
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.py"
        },
        {
            "src": "/api",
            "dest": "/api/index.py"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}