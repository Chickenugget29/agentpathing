{
    "version": 2,
    "builds": [
        {
            "src": "api/*.py",
            "use": "@vercel/python@4.0.0"
        },
        {
            "src": "web/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/analyze",
            "dest": "/api/analyze.py"
        },
        {
            "src": "/api/history",
            "dest": "/api/history.py"
        },
        {
            "src": "/api/task",
            "dest": "/api/task.py"
        },
        {
            "src": "/api/override",
            "dest": "/api/override.py"
        },
        {
            "src": "/api/patterns",
            "dest": "/api/patterns.py"
        },
        {
            "src": "/api/?$",
            "dest": "/api/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "/front_end/$1"
        }
    ]
}